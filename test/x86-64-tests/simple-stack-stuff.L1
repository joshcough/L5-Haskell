((
  (esp -= 32)    ;; save space on the stack for 3 temporaries

  ; put something on the stack
  ((mem esp 0) <- 3)
  ((mem esp 8) <- 85)
  ((mem esp 16) <- 109)

  ; set up the stack pointer to get something off the stack, i think...
  (edx <- (mem esp 0))
  (eax <- (print edx))
  (edx <- (mem esp 8))
  (eax <- (print edx))
  (edx <- (mem esp 16))
  (eax <- (print edx))

  (esp += 32)
))
