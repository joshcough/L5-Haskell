(((rdx <- 10)
  (call :fib_rec)
  (rax *= 2)
  (rax += 1)
  (rax <- (print rax)))

(:fib_rec
 (arg <- rdx)
 (cjump arg <= 1 :base :recurse)
 :base
 (rax <- 1)
 (return)

 :recurse
 (n1 <- arg)
 (n1 -= 1)
 (n2 <- arg)
 (n2 -= 2)

 (rdx <- n1)
 (call :fib_rec)
 (sum <- rax)

 (rdx <- n2)
 (call :fib_rec)
 (sum += rax)
 
 (rax <- sum)
 (return)))