(
 (
  (rax <- 3)   ;; exponent
  (rsi <- 5)   ;; base
  (call :exp) 
  (rax <- rsi)
  (rax += rax)
  (rax += 1)
  (rax <- (print rax))
 )

 (:exp
  (esp -= 4)
  ((mem ebp -4) <- rsi)

  :loop
  (cjump 1 < rax :keep_going :done)

  :keep_going
  (rax -= 1)
  (rbx <- (mem ebp -4))
  (rsi *= rbx)
  (goto :loop)

  :done
  (esp += 4)
  (return)
 )
)   
