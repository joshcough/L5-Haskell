((
  (rax <- (allocate 9 3)) ; size 4 array of 1's. you can only allocate into rax.
  (rdi <- rax) ; save the array pointer in rdi

  ((mem rdi 4) <- 5) ; set the first element to 2
  ((mem rdi 8) <- 7) ; set the first element to 3
  ((mem rdi 12) <- 9) ; set the first element to 4
  ((mem rdi 16) <- 11) ; set the first element to 5

  ; print the elements in the array

  (rsi <- (mem rdi 4)) ; put the 1st element into rdi
  (rax <- (print rsi)) ; print it

  (rsi <- (mem rdi 8)) ; put the 1st element into rdi
  (rax <- (print rsi)) ; print it

  (rsi <- (mem rdi 12)) ; put the 1st element into rdi
  (rax <- (print rsi)) ; print it

  (rsi <- (mem rdi 16)) ; put the 1st element into rdi
  (rax <- (print rsi)) ; print it
))
