((
  (esp -= 12)    ;; save space on the stack for 3 temporaries

  ; put something on the stack
  ((mem ebp -4) <- 3)
  ((mem ebp -8) <- 85)
  ((mem ebp -12) <- 109)

  ; set up the stack pointer to get something off the stack, i think...
  (rdx <- (mem ebp -4))
  (rax <- (print rdx))
  (rdx <- (mem ebp -8))
  (rax <- (print rdx))
  (rdx <- (mem ebp -12))
  (rax <- (print rdx))

  (esp += 12)
))
