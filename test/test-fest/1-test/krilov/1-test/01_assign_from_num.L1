; Test assigning literals to registers
((
  (rax <- 15)
  (rax <- (print rax))   ; 7
  
  (rcx <- 9)
  (rax <- (print rcx))   ; 4
    
  ; 0
  (rsi <- 0)
  (rax <- (print rax))   ; 0
  
  ; maximum encoded number: 2^31-1
  (rdi <- 2147483647)
  (rax <- (print rdi))   ; 1073741823
  
  ; -1
  (rax <- -1)
  (rax <- (print rax))   ; -1
  
  ; -2
  (rax <- -3)
  (rax <- (print rax))   ; -2
  
  ; minimum encoded number: -2^31+1 (assuming since -2^32 is even, it means it would be interpreted as an array)
  (rax <- -2147483647)
  (rax <- (print rax))   ; -1073741824
))