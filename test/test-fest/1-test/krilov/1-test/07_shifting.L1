; Test shifting operators
((
  ; Test left shift by 1 bit
  (rax <- 3)  ; 011
  (rax <<= 1) ; 110
  (rax += 1)  ; 111 = 7_encoded = 3
  (rax <- (print rax))  ; 3
  
  ; Test left shift by 3 bits
  (rax <- 3)  ; 011
  (rax <<= 3) ; 11000
  (rax += 1)  ; 11001 = 25_encoded = 12
  (rax <- (print rax))  ; 12
  
  ; Test right shift by 1 bit
  (rax <- 6)  ; 110
  (rax >>= 1) ; 011 = 3_encoded = 1
  (rax <- (print rax))  ; 1
  
  ; Test right shift by 2 bits
  (rax <- 20) ; 10100
  (rax >>= 2) ; 00101 = 5_encoded = 2
  (rax <- (print rax))  ; 2
  
))