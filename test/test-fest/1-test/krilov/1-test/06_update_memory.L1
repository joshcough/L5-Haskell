; Test updating memory
((
  ; Allocate the array [5 5 5], then change the second value to 7, and print the array.
  (rax <- (allocate 7 11))
  (rdi <- rax)
  ((mem rdi 8) <- 15)
  (rax <- (mem rdi 4))
  (rax <- (print rax))   ; 5
  (rax <- (mem rdi 8))
  (rax <- (print rax))   ; 7
  (rax <- (mem rdi 12))
  (rax <- (print rax))   ; 5
  ))))