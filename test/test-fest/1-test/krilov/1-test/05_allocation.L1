; Test allocating & initializing memory
((
  ; Allocate the array [5 5 5] and print each value individually
  (rax <- (allocate 7 11))
  (rdi <- rax)
  (rax <- (mem rdi 4))
  (rax <- (print rax))   ; 5
  (rax <- (mem rdi 8))
  (rax <- (print rax))   ; 5
  (rax <- (mem rdi 12))
  (rax <- (print rax))   ; 5
  ))